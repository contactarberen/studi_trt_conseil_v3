{% extends 'base.html.twig' %}

{% block title %}Candidature{% endblock %}

{% block body %}
    <h1>Candidature</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ candidature.id }}</td>
            </tr>
            <tr>
                <th>IsValid</th>
                <td>{{ candidature.isValid ? 'Yes' : 'No' }}</td>
            </tr>
             <tr>
                <th>User Id</th>
                <td>{{ candidature.userId }}</td>
            </tr>
            <tr>
                <th>Annonce Id</th>
                <td>{{ candidature.annonceId }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_candidature_index') }}">back to list</a>

    <a href="{{ path('app_candidature_edit', {'id': candidature.id}) }}">edit</a>

    {{ include('candidature/_delete_form.html.twig') }}
{% endblock %}
